	measure 'Validacion Laboral' = ```
			
			VAR PEA_Total = SUM(censo_ageb_2020_nacional_coordenadas[PEA])
			VAR Ocupados = SUM(censo_ageb_2020_nacional_coordenadas[POCUPADA])  
			VAR Desocupados = SUM(censo_ageb_2020_nacional_coordenadas[PDESOCUP])
			VAR SumaPEA = Ocupados + Desocupados
			VAR DiferenciaPct = IF(PEA_Total > 0, ABS(PEA_Total - SumaPEA) / PEA_Total, 1)
			VAR OcupadosMayorPEA = Ocupados > PEA_Total
			RETURN
			IF(
			    PEA_Total > 0,
			    SWITCH(
			        TRUE(),
			        OcupadosMayorPEA, "🚨 OCUPADOS > PEA",
			        DiferenciaPct <= 0.05, "✅ VÁLIDO",
			        DiferenciaPct <= 0.15, "⚠️ REVISAR", 
			        "❌ INCONSISTENTE"
			    ),
			    "📊 SIN PEA"
			)
			
			
