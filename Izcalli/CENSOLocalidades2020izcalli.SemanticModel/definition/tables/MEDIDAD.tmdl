	measure Prioridad_Intervencion = ```
			
			VAR VulnScore = IF([Vulnerabilidad_Social] <> BLANK(), [Vulnerabilidad_Social], 0)
			VAR ServiciosScore = IF([Servicios_Completos] <> BLANK(), [Servicios_Completos], 100)
