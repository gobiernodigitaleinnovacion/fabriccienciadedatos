	measure A√±os_Bono_Demografico = ```
			
			VAR BonoActual = [Bono_Demografico]
			VAR PoblacionPEA = [POB_15_64]
			VAR PoblacionTotal = [Poblacion_Total]
			
			// Calculamos el ratio de dependencia demogr√°fica
			VAR RatioDependencia = 
			    DIVIDE(
			        PoblacionTotal - PoblacionPEA,
			        PoblacionPEA,
			        0
			    ) * 100
			
			// Proyecci√≥n m√°s realista
			VAR AnosRestantesBono = 
			    IF(
			        RatioDependencia < 50,
			        (50 - RatioDependencia) * 0.5,
			        0
			    )
			
			RETURN 
