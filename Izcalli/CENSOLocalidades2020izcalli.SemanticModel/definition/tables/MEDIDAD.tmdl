	measure Alerta_Demografica_V2 = ```
			
			// 1. C√°lculo gradual de riesgos (sin cortes abruptos)
			VAR RiesgoEnvejecimiento = MIN([Indice_Envejecimiento] * 0.4, 25)  // 60 ‚Üí 24 pts
			VAR RiesgoMigracion = MIN([Vulnerabilidad_Social] * 0.46, 30)      // 65 ‚Üí 30 pts
			VAR RiesgoDigital = MIN([Brecha_Digital] * 0.27, 20)               // 75 ‚Üí 20 pts
			VAR RiesgoEducativo = MIN([Tasa_Analfabetismo] * 2.5, 25)          // 10% ‚Üí 25 pts
			
			// 2. Suma total (m√°ximo posible = 100)
			VAR RiesgoTotal = RiesgoEnvejecimiento + RiesgoMigracion + RiesgoDigital + RiesgoEducativo
			
			// 3. Clasificaci√≥n con ajuste estad√≠stico (opcional)
			VAR Percentil75 = 60  // Ejemplo: El 75% de las regiones tienen RiesgoTotal < 60
			VAR Percentil50 = 40
			VAR Percentil25 = 20
			
			// 4. Retorno con niveles de alerta
			RETURN 
			    SWITCH(
			        TRUE(),
			        RiesgoTotal >= Percentil75, "üö® CRISIS INMINENTE",
			        RiesgoTotal >= Percentil50, "‚ö†Ô∏è ATENCI√ìN URGENTE",
			        RiesgoTotal >= Percentil25, "üü° MONITOREAR",
			        "‚úÖ ESTABLE"
			    )
			```
